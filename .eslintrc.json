{
    "env": {
        "commonjs": true,
        "es6": true,
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "react/react-in-jsx-scope": 0,
        "no-console": "error"
    }
}